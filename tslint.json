{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "array-type": [true, "generic"],
    "import-sources-order": false,
    "interface-over-type-literal": false,
    "jsx-no-lambda": false,
    "member-access": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  }
}
