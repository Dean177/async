{
  "name": "abortable-networking",
  "dependencies": {
    "abortable": "^1.0.0-beta.0"
  },
  "peerDependencies": {
    "superagent": "^4.1.0"
  },
  "devDependencies": {
    "@types/jest": "24.9.0",
    "@types/superagent": "4.1.4",
    "jest": "25.1.0",
    "superagent": "5.2.1",
    "ts-jest": "24.3.0",
    "typescript": "3.7.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts}"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)test.ts"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    }
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/networking.js",
  "types": "dist/networking.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "dev": "tsc --noEmit --watch",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "test:coverage": "jest  --coverage",
    "typecheck": "tsc --noEmit"
  },
  "version": "1.0.0-beta.2"
}
