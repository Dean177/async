{
  "name": "abortable-networking",
  "dependencies": {
    "abortable": "^0.0.1",
    "lodash": "^4.17.4"
  },
  "peerDependencies": {
    "superagent": "^3.5.2"
  },
  "devDependencies": {
    "@types/jest": "20.0.5",
    "@types/lodash": "4.14.71",
    "@types/superagent": "3.5.3",
    "jest": "20.0.4",
    "superagent": "3.5.2",
    "ts-jest": "20.0.7",
    "typescript": "2.4.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/index.ts"
    ],
    "coverageReporters": [
      "text"
    ],
    "mapCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "license": "MIT",
  "main": "index.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:dev": "tsc --noEmit --pretty --watch",
    "dev": "concurrently -n \"make,test\" \"npm run build:dev\" \"npm run test:dev\"",
    "test": "jest --coverage",
    "test:dev": "jest --watch",
    "prepublish": "npm run test && npm run build"
  },
  "version": "0.0.1"
}
