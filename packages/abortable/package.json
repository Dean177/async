{
  "author": "Dean Merchant",
  "name": "abortable",
  "devDependencies": {
    "@types/jest": "23.3.13",
    "jest": "23.6.0",
    "ts-jest": "23.10.5",
    "typescript": "3.3.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts}"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)test.ts"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    }
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "dev": "tsc --noEmit --watch",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "version": "0.7.0"
}
