{
  "author": "Dean Merchant",
  "name": "abortable",
  "devDependencies": {
    "@types/jest": "24.9.0",
    "jest": "25.1.0",
    "ts-jest": "24.3.0",
    "typescript": "3.7.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts}"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)test.ts"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    }
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "dev": "tsc --noEmit --watch",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "version": "1.0.0-beta.2"
}
