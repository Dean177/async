{
  "dependencies": {
    "abortable": "^0.7.0",
    "lerna": "3.11.1",
    "lint-staged": "8.1.4",
    "rimraf": "2.6.3"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.4",
    "@types/prop-types": "15.5.9",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.8.1",
    "jest": "24.1.0",
    "props-interceptor": "1.0.1",
    "raf": "3.4.1",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-test-renderer": "16.8.1",
    "react-testing-library": "5.8.0",
    "ts-jest": "23.10.5",
    "typescript": "3.3.3"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/index.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)test.ts?(x)"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    }
  },
  "main": "dist/withAsync.js",
  "types": "dist/withAsync.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:docs": "rm -rf ../../docs && cd example && yarn && yarn build && cp -r ./build ../../../docs",
    "dev": "tsc --noEmit --watch",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "name": "with-async",
  "version": "1.0.0"
}
